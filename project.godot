; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="RPG"
run/main_scene="res://scene/main.tscn"
config/features=PackedStringArray("4.4", "Forward Plus")
config/icon="res://icon.svg"

[autoload]

NavigationManager="*res://global/navigation_manager.gd"
TransitionScreen="*res://global/transition_screen.tscn"
global="*res://陷阱/global.gd"
Dialogic="*res://addons/dialogic/Core/DialogicGameHandler.gd"
Cursor="*res://global/cursor.tscn"
EventBus="*res://block/event_bus.gd"
InGameUi="*res://menu/in_game_ui.tscn"

[dialogic]

directories/dch_directory={
"cat_head": "res://dialogue/npc/cat_head.dch",
"cave_girl": "res://dialogue/npc/cave_girl.dch",
"fish_告示牌": "res://dialogue/npc/fish_告示牌.dch",
"frog_man": "res://dialogue/npc/frog_man.dch",
"main_slime": "res://dialogue/npc/main_slime.dch",
"unit_test_character": "res://Tests/Resources/unit_test_character.dch"
}
directories/dtl_directory={
"first_main": "res://dialogue/timeline/first_main.dtl",
"hidden_slime_repeat": "res://dialogue/timeline/hidden_slime_repeat.dtl",
"hidden_slime_talk": "res://dialogue/timeline/hidden_slime_talk.dtl",
"home_fishing": "res://dialogue/timeline/home_fishing.dtl",
"main_cathead": "res://main_cathead.dtl",
"main_cathead_repeat": "res://dialogue/timeline/main_cathead_repeat.dtl",
"main_frogman_talk": "res://dialogue/timeline/main_frogman_talk.dtl",
"main_slime_talk": "res://dialogue/timeline/main_slime_talk.dtl",
"main_tip": "res://dialogue/timeline/main_tip.dtl",
"progress2_frogman": "res://dialogue/timeline/progress2_frogman.dtl",
"progress3_cathead": "res://dialogue/timeline/progress3_cathead.dtl",
"progress3_cathead_repeat": "res://dialogue/timeline/progress3_cathead_repeat.dtl",
"progress4_cavegirl": "res://dialogue/timeline/progress4_cavegirl.dtl",
"progress4_cavegirl_repeat": "res://dialogue/timeline/progress4_cavegirl_repeat.dtl"
}
layout/style_directory={
"New_File": "res://dialogue/style/底下對話框.tres",
"tip": "res://dialogue/style/tip.tres",
"角色對話": "res://dialogue/style/角色對話.tres"
}
layout/style_list=["res://dialogue/style/底下對話框.tres", "res://dialogue/style/角色對話.tres", "res://dialogue/style/tip.tres"]
layout/default_style="res://dialogue/style/New_File.tres"
variables={}
extensions_folder="res://addons/dialogic"
text/letter_speed=0.01
text/initial_text_reveal_skippable=true
text/text_reveal_skip_delay=0.1
text/advance_delay=0.1
text/autoadvance_per_character_delay=0.1
text/autoadvance_ignored_characters_enabled=true
animations/join_default_length=0.5
animations/join_default_wait=true
animations/leave_default_length=0.5
animations/leave_default_wait=true
animations/cross_fade_default_length=0.5
choices/autofocus_first=true
choices/delay=0.2
choices/reveal_delay=0
choices/reveal_by_input=false
save/autosave_delay=60.0
save/encryption_on_exports_only=true
text/autopauses={}
audio/channel_defaults={
"": {
"audio_bus": "",
"fade_length": 0.0,
"loop": false,
"volume": 0.0
},
"music": {
"audio_bus": "",
"fade_length": 0.0,
"loop": true,
"volume": 0.0
}
}
text/input_action="dialogic_default_action"
text/autocolor_names=false

[dialogue_manager]

editor/new_file_template="~ this_is_a_node_title
Nathan: [[Hi|Hello|Howdy]], this is some dialogue.
Nathan: Here are some choices.
- First one
	Nathan: You picked the first one.
- Second one
	Nathan: You picked the second one.
- Start again => this_is_a_node_title
- End the conversation => END
Nathan: For more information see the online documentation.
=> END"

[display]

window/size/viewport_width=288
window/size/viewport_height=162
window/size/borderless=true
window/size/window_width_override=960
window/size/window_height_override=540
window/stretch/mode="viewport"
window/stretch/scale_mode="integer"
window/per_pixel_transparency/allowed=true
mouse_cursor/custom_image="uid://ccrce3pn3cen8"

[editor_plugins]

enabled=PackedStringArray("res://addons/dialogic/plugin.cfg")

[file_customization]

folder_colors={
"res://assets/": "orange",
"res://dialogue/": "yellow",
"res://global/": "blue",
"res://scenes/": "green",
"res://tileset/": "pink"
}

